  <!--====== Easypiechart ======-->
  <script src="/js/jquery.easypiechart.min.js" is:inline></script>
  <!--====== Nice Select ======-->
  <script src="/js/jquery.nice-select.min.js" is:inline></script>
  <!--====== Lity ======-->
  <script src="/js/lity.min.js" is:inline></script>
  <!--====== WOW Js ======-->
  <script src="/js/wow.min.js" is:inline></script>
  <!--====== Main JS ======-->
  <script src="/js/main.js" is:inline></script>

  </body>
</html>

